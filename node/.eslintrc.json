{
  "env": {
    "node": true,
    "es6": true,
    "jest": true,
    "amd": true
  },
  "parser": "babel-eslint",
  "plugins": ["import", "simple-import-sort"],
  "rules": {
    "accessor-pairs": [2, {"getWithoutSet": false, "setWithoutGet": false}],
    "array-bracket-spacing": 2,
    "arrow-parens": [2, "as-needed"],
    "arrow-spacing": 2,
    "block-scoped-var": 2,
    "brace-style": 2,
    "callback-return": 2,
    "camelcase": 1,
    "class-methods-use-this": "off",
    "comma-dangle": [2, "always-multiline"],
    "comma-spacing": 2,
    "comma-style": 2,
    "complexity": 2,
    "computed-property-spacing": 2,
    "consistent-return": 2,
    "consistent-this": 2,
    "constructor-super": 2,
    "curly": [2, "all"],
    "default-case": 2,
    "dot-location": [2, "property"],
    "dot-notation": 2,
    "eol-last": 2,
    "eqeqeq": 2,
    "func-names": 1,
    "func-style": "off",
    "generator-star-spacing": 2,
    "guard-for-in": 2,
    "handle-callback-err": 2,
    "indent": 2,
    "init-declarations": [2, "always"],
    "key-spacing": 2,
    "keyword-spacing": 2,
    "linebreak-style": 2,
    "lines-around-comment": 2,
    "max-depth": 2,
    "max-nested-callbacks": 2,
    "max-params": "warn",
    "new-cap": 2,
    "no-alert": 2,
    "no-array-constructor": 2,
    "no-caller": 2,
    "no-catch-shadow": 2,
    "no-class-assign": 2,
    "no-cond-assign": [2, "always"],
    "no-const-assign": 2,
    "no-constant-condition": 2,
    "no-continue": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-div-regex": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-else-return": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": [2, "all"],
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-func-assign": 2,
    "no-implicit-coercion": 2,
    "no-implied-eval": 2,
    "no-inline-comments": 2,
    "no-inner-declarations": [2, "both"],
    "no-invalid-regexp": 2,
    "no-invalid-this": 2,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-lonely-if": 2,
    "no-mixed-requires": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-multiple-empty-lines": 2,
    "no-native-reassign": 2,
    "no-negated-in-lhs": 2,
    "no-nested-ternary": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-require": 2,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-param-reassign": 2,
    "no-path-concat": 2,
    "no-process-env": 1,
    "no-process-exit": 1,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow": ["error", {"builtinGlobals": false, "hoist": "functions", "allow": []}],
    "no-shadow-restricted-names": 2,
    "no-spaced-func": 2,
    "no-sparse-arrays": 2,
    "no-sync": 2,
    "no-ternary": 2,
    "no-this-before-super": 2,
    "no-throw-literal": 2,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-undefined": 1,
    "no-underscore-dangle": 0,
    "no-unexpected-multiline": 2,
    "no-unneeded-ternary": 2,
    "no-unreachable": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": "off",
    "no-use-before-define": 2,
    "no-useless-call": 2,
    "no-var": 2,
    "no-void": 2,
    "no-warning-comments": 2,
    "no-with": 2,
    "object-curly-spacing": [2, "always"],
    "object-shorthand": 2,
    "one-var": ["error", "never"],
    "operator-assignment": 2,
    "operator-linebreak": 2,
    "padded-blocks": "off",
    "prefer-reflect": 2,
    "prefer-spread": 2,
    "quote-props": [2, "as-needed"],
    "quotes": [2, "single"],
    "radix": 2,
    "require-yield": 2,
    "semi": 2,
    "semi-spacing": 2,
    "semi-style": 2,
    "sort-vars": 2,
    "space-before-blocks": 2,
    "space-before-function-paren": ["error", "never"],
    "space-in-parens": 2,
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "spaced-comment": 2,
    "use-isnan": 2,
    "valid-jsdoc": 2,
    "valid-typeof": 2,
    "vars-on-top": 2,
    "wrap-iife": 2,
    "yoda": 2,
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "no-empty-function": "off",
    "prefer-rest-params": "error",
    "prefer-const": ["error", {"destructuring": "any", "ignoreReadBeforeAssign": true}],
    "simple-import-sort/sort": "error"
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
